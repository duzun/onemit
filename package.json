{
  "name": "onemit",
  "version": "2.0.3",
  "description": "Event emitter",
  "keywords": [
    "onemit",
    "emitter",
    "events"
  ],
  "reveal": true,
  "author": {
    "name": "Dumitru Uzun",
    "email": "contact@duzun.me",
    "url": "https://DUzun.Me/"
  },
  "readmeFilename": "README.md",
  "license": [
    {
      "type": "MIT",
      "url": "https://github.com/duzun/onemit/raw/master/LICENSE"
    }
  ],
  "bugs": {
    "url": "https://github.com/duzun/onemit/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/duzun/onemit.git"
  },
  "main": "dist/onemit.js",
  "module": "onemit.js",
  "esnext": "onemit.js",
  "jsnext:main": "onemit.js",
  "unpkg": "dist/onemit.min.js",
  "scripts": {
    "build": "npm test && gccs --compilation_level WHITESPACE_ONLY --formatting pretty_print -- onemit.js dist/onemit.js && gccs dist/onemit.js",
    "verup": "verup",
    "test": "mocha --require should --reporter spec"
  },
  "files": [
    "onemit.js"
  ],
  "verup": {
    "files": [
      "onemit.js",
      "bower.json"
    ]
  },
  "devDependencies": {
    "gccs": "^1.3",
    "mocha": "^6.2",
    "should": "^13.2",
    "verup": "^1.7"
  },
  "jshintConfig": {
    "laxbreak": true,
    "laxcomma": true,
    "undef": true,
    "unused": true,
    "sub": true,
    "expr": true,
    "bitwise": false,
    "eqeqeq": false,
    "boss": true,
    "eqnull": true,
    "scripturl": true,
    "-W041": false
  }
}
